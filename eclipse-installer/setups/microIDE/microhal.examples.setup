      <predicate
          xsi:type="predicates:NamePredicate"
          pattern="serialPort"/>
      <predicate
          xsi:type="predicates:NamePredicate"
          pattern="signal slot"/>
      <predicate
          xsi:type="predicates:NamePredicate"
