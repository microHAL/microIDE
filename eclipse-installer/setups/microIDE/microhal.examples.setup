    </workingSet>
    <workingSet
        name="Other"
        id="">
      <predicate
          xsi:type="workingsets:ExclusionPredicate"
          excludedWorkingSet="//@setupTasks.3/@workingSets.1 //@setupTasks.3/@workingSets.0 //@setupTasks.3/@workingSets.2 //@setupTasks.3/@workingSets.3"/>
    </workingSet>
  </setupTask>
  <setupTask
      xsi:type="git:GitCloneTask"
      id="git.clone.microhal"
      remoteURI="microHAL/microhal"
      pushURI=""
      recursive="true">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>github.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>Github Access</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="examples"
      name="examples">
    <setupTask
        xsi:type="projects:ProjectsImportTask"
        id="org.eclipse.example-diagnostic.project.import">
      <sourceLocator
          rootFolder="${git.clone.microhal.location}/examples"
          locateNestedProjects="true"/>
      <description>Import the diagnostic module example in the workspace</description>
    </setupTask>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      name="devices.examples">
    <setupTask
        xsi:type="projects:ProjectsImportTask"
        id="org.eclipse.deviceexample-bmp180.project.import">
      <sourceLocator
          rootFolder="${git.clone.microhal.location}/drivers"
          locateNestedProjects="true"/>
      <description></description>
    </setupTask>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsImportTask">
    <sourceLocator
        rootFolder="${git.clone.microhal.location}/components"
        locateNestedProjects="true"/>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsImportTask">
    <sourceLocator
        rootFolder="${git.clone.microhal.location}/tests"
        locateNestedProjects="true"/>
  </setupTask>
  <stream name="master"
      label="Master"/>
  <stream name="devel"
      label="Devel"/>
  <stream name="port-stm32l4xx"
      label="Port-stm32l4xx"/>
  <stream name="port-stm32f3xx"
